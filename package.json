{
  "name": "koa-skeleton",
  "description": "a simple, full-featured koa application made to be forked and modified",
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/danneu/koa-skeleton"
  },
  "scripts": {
    "start": "node ./babel.app",
    "start-dev": "nodemon --ext js,html ./babel.app",
    "reset-db": "node ./babel.reset_db",
    "deploy": "git push heroku master"
  },
  "engines": {
    "node": "6.x"
  },
  "dependencies": {
    "autolinker": "^1.2.1",
    "babel-core": "^6.17.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "bcryptjs": "2.3.0",
    "better-assert": "1.0.2",
    "debug": "2.2.0",
    "dotenv": "^2.0.0",
    "interval-cache": "0.0.1",
    "knex": "^0.12.6",
    "koa": "^2.0.0",
    "koa-better-static": "^1.0.5",
    "koa-body": "^1.6.0",
    "koa-bouncer": "^6.0.0",
    "koa-compress": "^1.0.9",
    "koa-convert": "^1.2.0",
    "koa-helmet": "^2.0.0",
    "koa-logger": "1.3.0",
    "koa-nunjucks-render": "1.1.1",
    "koa-router": "^7.0.1",
    "nunjucks": "^2.5.2",
    "pg": "^6.1.0",
    "pg-extra": "0.0.9",
    "recaptcha-validator": "^1.0.6",
    "uuid": "^2.0.2"
  },
  "devDependencies": {
    "eslint": "^1.8.0",
    "eslint-config-volox": "^1.0.2"
  },
  "license": "MIT"
}
