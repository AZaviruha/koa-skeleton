    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "bcryptjs": "2.3.0",
    "better-assert": "1.0.2",
    "co": "4.6.0",
    "debug": "2.2.0",
    "dotenv": "^2.0.0",
    "interval-cache": "0.0.1",
